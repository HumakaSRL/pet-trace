{
    "hosting": {
        "public": "public",
        "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
        "headers": [
            {
                "source": "**/*.{css,js,png,jpg,jpeg,gif,svg}",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "public, max-age=2592000"
                    }
                ]
            }
        ]
    }
}
